<!--index.wxml-->
<view class="container">
    <view>
        <swiper class="swiper_box" indicator-dots="{{indicatorDots}}" vertical="{{vertical}}"
                autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" bindchange="swiperchange">
            <block wx:for="{{images}}">
                <swiper-item>
                    <image src="{{item.img}}" class="slide-image" mode="scaleToFill"/>
                </swiper-item>
            </block>
        </swiper>
    </view>

     <!-- <view class="text">
        <view class="line_flag"></view>
        <text></text>
    </view> -->
    
    <view class="venues_box">
        <view class="venues_list">
            <block wx:for="{{venuesItems}}">
                <view class="venues_item" data-id="{{item.menuName}}" bindtap='onNaviTo'>
                    <!-- <navigator url="../stuDetail/student?id={{item.id}}"> -->
                        <image src="{{item.smallpic}}"/>
                    <!-- </navigator> -->
                </view>
            </block>
        </view>
    </view>

    <view class="text">
        <view class="line_flag" style="padding-top:5px;width:113px; height:30px; background-repeat:no-repeat; background-image: url('data:image/png;base64,');">
           <text class='section'>驾培新闻</text>
        </view>
    </view>

    <view class="choice_box">
        <view class="choice_list">
            <block wx:for="{{choiceItems}}">
                <navigator url="../details/index?id={{item.id}}">
                    <view class="choice_item">
                        <view class='imageView'>
                          <image src="{{item.smallpic}}"></image>
                        </view>
                        <view class='infoDetail'>
                          <text class='newsTitle'>{{item.title}}</text>
                          <view class='detialView'>
                            <text class='category'>{{item.category}}</text>
                            <text class='date'>{{item.date}}</text>
                          </view>
                        </view>

                        <!-- <image src="{{item.goodspics}}"/>
                        <view class="title">{{item.title}}</view> -->
                    </view>
                </navigator>
            </block>
        </view>
    </view>

    <loading hidden="{{loadingHidden}}">
        加载中...
    </loading> 

</view>
